@model List<ResultStaffDTO>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-sm-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@Url.Content($"~/hotel-html-template/img/{item.Link1}")" class="rounded-circle img-fluid" alt="@item.Name" style="width: 100px; height: 100px; object-fit: cover;">
                        <h5 class="mt-3 mb-1">@item.Name</h5>
                        <p class="m-0 text-muted">@item.Title</p>
                    </div>
                </div>
            </div>
        </div>
    }
</div>
